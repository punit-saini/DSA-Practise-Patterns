#include <iostream>
using namespace std;
int main() {
  char a = 97;
  cout << a + 1 << endl;
  cout << "Enter n " << endl;
  int n, newn;
  cin >> n;
  for (int i = 1; i <= n; i++) {
    for (int j = 1; j <= n; j++) {
      newn = a + 1;
      cout << " " << a << " ";
      a = newn;
    }
    cout << endl;
    a = 97;
  }
}
