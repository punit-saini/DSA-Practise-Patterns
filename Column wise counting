#include <iostream>
using namespace std;

int main() {

  cout << "Enter number of columns " << endl;
  int n;
  cin >> n;
  int j = 1;
  int i = 1;
  int count = 1;

  while (i <= n) {
    while (j <= n) {
      cout << " " << count << " ";
      count++;
      j++;
    }
    cout << endl;
    j = 1;
    i++;
  }
}
